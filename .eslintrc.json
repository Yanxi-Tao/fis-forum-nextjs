{
  "extends": ["next", "next/core-web-vitals", "eslint:recommended", "plugin:prettier/recommended"],
  "rules": {
    "prettier/prettier": [
      "warn",
      {
        "trailingComma": "all",
        "tabWidth": 2,
        "semi": false,
        "singleQuote": true,
        "printWidth": 120,
        "bracketSpacing": true
      }
    ]
  }
}
